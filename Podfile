# Uncomment the next line to define a global platform for your project
 platform :ios, '13.0'

target 'FaceBook Login' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FirebaseDemo
   pod 'Firebase/Core'
   pod 'Firebase/Auth'
  
   # Pods for Facebook
 
   pod 'FBSDKCoreKit'
   pod 'FBSDKLoginKit'
   
end
